### testing rules

# test: run all tests in cwd and subdirs
@BUILD_TESTS_TRUE@test: test-cwd test-recurse
# test-cwd: run tests in cwd
@BUILD_TESTS_TRUE@test-cwd: ${TEST_PROGS}
@BUILD_TESTS_TRUE@	@[ -z "$(TEST_PROGS)" ] || { set -e; $(TESTS_ENVIRONMENT) ${GTESTER} --verbose ${TEST_PROGS}; }

# test-recurse: run tests in subdirs
@BUILD_TESTS_TRUE@test-recurse:
@BUILD_TESTS_TRUE@	@ for subdir in $(SUBDIRS) ; do \
@BUILD_TESTS_TRUE@	    test "$$subdir" = "." \
@BUILD_TESTS_TRUE@			-o "$$subdir" = "config" \
@BUILD_TESTS_TRUE@			-o "$$subdir" = "doc" \
@BUILD_TESTS_TRUE@			-o "$$subdir" = "manual" \
@BUILD_TESTS_TRUE@			-o "$$subdir" = "m4" \
@BUILD_TESTS_TRUE@			-o "$$subdir" = "po" \
@BUILD_TESTS_TRUE@			-o "$$subdir" = "tools" \
@BUILD_TESTS_TRUE@			|| \
@BUILD_TESTS_TRUE@	    ( cd $$subdir && $(MAKE) $(AM_MAKEFLAGS) test ) || exit $? ; \
@BUILD_TESTS_TRUE@	  done
# test-report: run tests in subdirs and generate report
# perf-report: run tests in subdirs with -m perf and generate report
# full-report: like test-report: with -m perf and -m slow
@BUILD_TESTS_TRUE@test-report perf-report full-report:	${TEST_PROGS}
@BUILD_TESTS_TRUE@	@ ignore_logdir=true ; \
@BUILD_TESTS_TRUE@	  if test -z "$$GTESTER_LOGDIR" ; then \
@BUILD_TESTS_TRUE@	    GTESTER_LOGDIR=`mktemp -d "\`pwd\`/.testlogs-XXXXXX"`; export GTESTER_LOGDIR ; \
@BUILD_TESTS_TRUE@	    ignore_logdir=false ; \
@BUILD_TESTS_TRUE@	  fi ; \
@BUILD_TESTS_TRUE@	  for subdir in $(SUBDIRS) ; do \
@BUILD_TESTS_TRUE@	    test "$$subdir" = "." -o "$$subdir" = "po" -o "$$subdir" = "po-properties" || \
@BUILD_TESTS_TRUE@	    ( cd $$subdir && $(MAKE) $(AM_MAKEFLAGS) $@ ) || exit $? ; \
@BUILD_TESTS_TRUE@	  done ; \
@BUILD_TESTS_TRUE@	  test -z "${TEST_PROGS}" || { \
@BUILD_TESTS_TRUE@	    case $@ in \
@BUILD_TESTS_TRUE@	    test-report) test_options="-k";; \
@BUILD_TESTS_TRUE@	    perf-report) test_options="-k -m=perf";; \
@BUILD_TESTS_TRUE@	    full-report) test_options="-k -m=perf -m=slow";; \
@BUILD_TESTS_TRUE@	    esac ; \
@BUILD_TESTS_TRUE@	    set -e; \
@BUILD_TESTS_TRUE@	    if test -z "$$GTESTER_LOGDIR" ; then \
@BUILD_TESTS_TRUE@	      ${GTESTER} --verbose $$test_options -o test-report.xml ${TEST_PROGS} ; \
@BUILD_TESTS_TRUE@	    elif test -n "${TEST_PROGS}" ; then \
@BUILD_TESTS_TRUE@	      ${GTESTER} --verbose $$test_options -o `mktemp "$$GTESTER_LOGDIR/log-XXXXXX"` ${TEST_PROGS} ; \
@BUILD_TESTS_TRUE@	    fi ; \
@BUILD_TESTS_TRUE@	  }; \
@BUILD_TESTS_TRUE@	  $$ignore_logdir || { \
@BUILD_TESTS_TRUE@	    echo '<?xml version="1.0"?>' > $@.xml ; \
@BUILD_TESTS_TRUE@	    echo '<report-collection>'  >> $@.xml ; \
@BUILD_TESTS_TRUE@	    for lf in `ls -L "$$GTESTER_LOGDIR"/.` ; do \
@BUILD_TESTS_TRUE@	      sed '1,1s/^<?xml\b[^>?]*?>//' <"$$GTESTER_LOGDIR"/"$$lf" >> $@.xml ; \
@BUILD_TESTS_TRUE@	    done ; \
@BUILD_TESTS_TRUE@	    echo >> $@.xml ; \
@BUILD_TESTS_TRUE@	    echo '</report-collection>' >> $@.xml ; \
@BUILD_TESTS_TRUE@	    rm -rf "$$GTESTER_LOGDIR"/ ; \
@BUILD_TESTS_TRUE@	    ${GTESTER_REPORT} --version 2>/dev/null 1>&2 ; test "$$?" != 0 || ${GTESTER_REPORT} $@.xml >$@.html ; \
@BUILD_TESTS_TRUE@	  }
@BUILD_TESTS_TRUE@.PHONY: test test-cwd test-recurse test-report perf-report full-report
# run make test-cwd as part of make check
@BUILD_TESTS_TRUE@check-local: test-cwd

